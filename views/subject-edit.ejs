<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <%- include part/css %>
    <title></title>
  </head>
  <body>
    <%- include part/menu %>

    <div class="col-md-2 col-md-offset-5">

      <form class="form-inline" action="/subjects/edit/<%= dataSubject.id %>" method="post">
        <label>Subject Name</label>
        <input type="text" name="subject_name" class="form-control" value="<%= dataSubject.subject_name %>">
        <br><br>

        <button type="submit" name="button" class="btn btn-info"><span class="glyphicon glyphicon-send"></span> &nbsp; Add Subject</button>
      </form>

    </div>
  </body>
</html>
